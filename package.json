{
  "name": "teat-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "server-install": "cd server && npm install",
    "install-all-deps": "concurrently \"npm run server-install\" \"npm run client-install\"",
    "buildclient": "cd client && npm run build",
    "buildserver": "cd server && npm run build",
    "build": "concurrently \"npm run install-all-deps\" && \"npm run buildclient\" && \"npm run buildserver\"",
    "start": "npm run --prefix server start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northlanee/test-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/northlanee/test-app/issues"
  },
  "homepage": "https://github.com/northlanee/test-app#readme",
  "dependencies": {
    "concurrently": "^8.2.0"
  }
}
